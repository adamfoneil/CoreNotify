@page "/"
@using CoreNotify.SerilogAlerts.SqlServer
@inject SerilogAlertService SerilogAlerts

<PageTitle>Home</PageTitle>

<h1>Hello, world!</h1>

Welcome to your new app.

<button @onclick="SendAlert">Test Serilog Alert</button>

@code {
	private async Task SendAlert()
	{
		await SerilogAlerts.TestAsync(100);
	}
}
